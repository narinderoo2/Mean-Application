    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ngx-build-plus": "^20.0.0",
    "postcss": "^8.5.6",
    "storybook": "^9.1.7",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.9.2"
  }
